owModal={showModal}\n      setShowModal={setShowModal}\n    >\n      <form onSubmit={handleSubmit(onSubmit)}>\n        <InputTextDefault name=\"id\" register={register} type=\"hidden\" />\n\n        <div className=\"flex flex-col gap-2 mb-4\">\n          <div className=\"flex gap-4\">\n            <InputTextDefault\n              label=\"Nama\"\n              name=\"name\"\n              register={register}\n              required\n              minLength={3}\n              errors={errors.name}\n              addClass=\"shrink w-[100%]\"\n            />\n          </div>\n          <div className=\"flex gap-4\">\n            <InputTextDefault\n              label=\"Link Github\"\n              name=\"link_github\"\n              register={register}\n              minLength={3}\n              addClass=\"shrink w-[100%]\"\n            />\n          </div>\n          <div className=\"flex gap-4\">\n            <InputTextDefault\n              label=\"Link Website\"\n              name=\"link_website\"\n              register={register}\n              minLength={3}\n              required\n              errors={errors.link_website}\n              addClass=\"shrink w-[100%]\"\n            />\n          </div>\n          <div className=\"flex gap-4\">\n            <InputTextDefault\n              label=\"Description\"\n              name=\"description\"\n              register={register}\n              minLength={3}\n              addClass=\"shrink w-[100%]\"\n            />\n          </div>\n          <ImageUpload\n            label=\"Description\"\n            name=\"image\"\n            register={register}\n            images={images}\n            setImages={setImages}\n            required\n          />\n        </div>\n        <div>\n          <ButtonDefault text=\"Simpan\" onClick={handleSubmit(onSubmit)} />\n        </div>\n      </form>\n    </ModalDefault>\n  );\n};\n\nexport default Form;\n","/media/smarthagar/Assets/Project/NextJS/UMUM/my-portfolio/src/app/adm